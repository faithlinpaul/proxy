{
  "request": {
    "method": "POST",
    "urlPathPattern": "/perf/user/([0-9]*)",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.msisdn"
      }
    ]
  },
  "response": {
    "status": 201,
    "fixedDelayMilliseconds" : 3000,
    "body": "{\"responseMsisdn\": \"$(msisdn)\"}",
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["body-transformer"]
  }
}