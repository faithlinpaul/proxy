{
  "request": {
    "method": "GET",
    "urlPathPattern": "/perf/delay/user/([0-9]*)"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "fixedDelayMilliseconds" : 2000,
    "body": "{\"data\": \"$(!RandomInteger)\"}",
    "transformers": ["body-transformer"]
  }
}